//Write a C program to find maximum number in array containing five elements of integer type




#include <stdio.h>

int main() {
    int elements[5];
    int max;

   
    for (int i = 0; i < 5; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &elements[i]);
    }

    
    max = elements[0];

   
    for (int i = 1; i < 5; i++) {
        if (elements[i] > max) {
            max = elements[i];
        }
    }

    
    printf("The maximum element in the array is: %d\n", max);

    return 0;
}



